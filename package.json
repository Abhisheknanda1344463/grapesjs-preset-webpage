{
  "name": "grapesjs-preset-webpage",
  "version": "0.1.3",
  "description": "GrapesJS Plugin Boilerplate",
  "main": "dist/grapesjs-preset-webpage.min.js",
  "scripts": {
    "lint": "eslint src",
    "v:patch": "npm version --no-git-tag-version patch",
    "build": "cross-env WEBPACK_ENV=prod && npm run v:patch && webpack",
    "start": "cross-env WEBPACK_ENV=dev ./node_modules/.bin/webpack-dev-server --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/artf/grapesjs-preset-webpage.git"
  },
  "keywords": [
    "grapesjs",
    "plugin",
    "boilerplate",
    "wysiwyg"
  ],
  "author": "Artur Arseniev",
  "license": "BSD-3-Clause",
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "peerDependencies": {
    "grapesjs": "0.x"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "cross-env": "^5.1.1",
    "eslint": "^4.1.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
